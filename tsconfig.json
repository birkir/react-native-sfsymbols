{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-native",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "outDir": "lib"
  },
  "exclude": ["node_modules", "lib"],
  "include": ["src"],
  "compileOnSave": false,
  "buildOnSave": false
}
